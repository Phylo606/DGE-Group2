<Window x:Class="DGE_Group_2_WPF_Wireframe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DGE_Group_2_WPF_Wireframe"
        mc:Ignorable="d"
        Title="Time Entered" Height="450" Width="800">
    <Grid>
        <Button x:Name="Time_Entered" Content="Time Entered" HorizontalAlignment="Left" Margin="24,28,0,0" VerticalAlignment="Top" Width="223" Click="Button_Click" Height="50" FontSize="16"/>
        <Button Content="Time Left" HorizontalAlignment="Left" Margin="281,28,0,0" VerticalAlignment="Top" Width="222" Height="50" FontSize="16"/>
        <Button Content="Reporting" HorizontalAlignment="Left" Margin="538,28,0,0" VerticalAlignment="Top" Width="225" Height="50" FontSize="16"/>
        <Label Content="User ID" HorizontalAlignment="Left" Margin="133,113,0,0" VerticalAlignment="Top" Width="69" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" Height="31" Margin="220,113,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="127" FontSize="16"/>
        <Label Content="Room" HorizontalAlignment="Left" Margin="144,185,0,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="1.173,2.18"/>
        <TextBox HorizontalAlignment="Left" Height="31" Margin="220,185,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="127" FontSize="16"/>
        <Label Content="Date and Time Left" HorizontalAlignment="Left" Margin="52,258,0,0" VerticalAlignment="Top" FontSize="16"/>
        <DatePicker HorizontalAlignment="Left" Margin="220,258,0,0" VerticalAlignment="Top" Height="31" Width="136" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" Height="31" Margin="380,258,0,0" TextWrapping="Wrap" Text="Hour" VerticalAlignment="Top" Width="47" TextChanged="TextBox_TextChanged_1" FontSize="16"/>
        <TextBox HorizontalAlignment="Left" Height="31" Margin="443,258,0,0" TextWrapping="Wrap" Text="Minute
                 " VerticalAlignment="Top" Width="60" TextChanged="TextBox_TextChanged_1" FontSize="16"/>
        <Label Content=":" HorizontalAlignment="Left" Margin="427,254,0,0" VerticalAlignment="Top" Height="31" Width="39" FontSize="16"/>
        <RadioButton Content="Class" HorizontalAlignment="Left" Margin="222,321,0,0" VerticalAlignment="Top"/>
        <RadioButton Content="Office" HorizontalAlignment="Left" Margin="319,321,0,0" VerticalAlignment="Top"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="76,343,0,0" VerticalAlignment="Top" Width="120" Height="31" FontSize="16"/>

    </Grid>
</Window>
